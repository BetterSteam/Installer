{
  "name": "millennium.installer.prod",
  "description": "Manages the installation of Millennium and its dependencies.",
  "private": true,
  "version": "1.0.8",
  "scripts": {
    "dev": "vite",
    "compile": "electron-webpack",
    "build": "tsc && vite build && electron-builder -p never",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "build": {
    "copyright": "Copyright Â© 2024 Millennium | Steam++",
    "productName": "Millennium",
    "appId": "scripter.millennium",
    "win": {
      "artifactName": "${productName}.Installer-Windows.${ext}",
      "icon": "src/assets/logo.ico",
      "target": {
        "target": "portable",
        "arch": [
          "ia32"
        ]
      }
    },
    "portable": {
      "requestExecutionLevel": "user",
      "useZip": true
    },
    "directories": {
      "output": "build/output",
      "app": ""
    }
  },
  "dependencies": {
    "@types/follow-redirects": "^1.14.4",
    "@types/ps-list": "^6.2.1",
    "axios": "^1.6.6",
    "child_process": "^1.0.2",
    "electron-webpack": "^2.8.2",
    "framer-motion": "^11.0.5",
    "is-running": "^2.1.0",
    "ps-list": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-tooltip": "^5.26.0",
    "react-transition-group": "^4.4.5",
    "regedit": "^5.1.2",
    "rehype-raw": "^7.0.0",
    "winreg": "^1.2.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^26.6.7",
    "electron-builder": "^24.6.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js"
}
